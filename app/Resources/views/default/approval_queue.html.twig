{% extends 'base.html.twig' %}

{% block page_title %}
 <title>Unpublished Datasets</title>
{% endblock %}

{% block content %}

<div class="col-xs-5">
 <div class="page-header">
 <h1>Unpublished Datasets</h1>
 </div>

 <ul>
 
 {% for item in approvalQueue %}
